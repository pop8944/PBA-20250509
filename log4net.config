<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<log4net>
		<root>
			<appender-ref ref="Total" />
			<appender-ref ref="AsyncTotalAppender" />
		</root>

		<logger name="Total" additivity="false">
			<level value="ALL" />
			<appender-ref ref="TotalAppender" />
		</logger>

		<appender name="TotalAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_Total.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%message%newline" />
			</layout>
		</appender>

		<logger name="Repeatability" additivity="false">
			<level value="ALL" />
			<appender-ref ref="RepeatabilityAppender" />
		</logger>
		<appender name="RepeatabilityAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_Repeatability.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%date{yyyy/MM/dd HH:mm:ss fff}], [TID:%thread]:, %message%newline" />
			</layout>
		</appender>

		<logger name="Exception" additivity="false">
			<level value="ALL" />
			<appender-ref ref="ExceptionAppender" />
		</logger>
		<appender name="ExceptionAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_Exception.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%date{yyyy/MM/dd HH:mm:ss fff}] [TID:%thread]: %message%newline" />
			</layout>
		</appender>

		<appender name="AsyncNormalAppender" type="Log4Net.Async.AsyncForwardingAppender,Log4Net.Async">
			<appender-ref ref="NormalAppender" />
			<bufferSize value="512" />
			<flushInterval value="1000" />
		</appender>

		<logger name="Normal" additivity="false">
			<level value="ALL" />
			<appender-ref ref="NormalAppender" />
		</logger>

		<appender name="NormalAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_Normal.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[TID:%thread]: %message%newline" />
			</layout>
		</appender>

		<logger name="System" additivity="false">
			<level value="ALL" />
			<appender-ref ref="SystemAppender" />
		</logger>
		<appender name="SystemAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_System.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%date{yyyy/MM/dd HH:mm:ss fff}] [TID:%thread]: %message%newline" />
			</layout>
		</appender>

		<logger name="Alarm" additivity="false">
			<level value="ALL" />
			<appender-ref ref="AlarmAppender" />
		</logger>
		<appender name="AlarmAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_Alarm.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%date{yyyy/MM/dd HH:mm:ss fff}] [TID:%thread]: %message%newline" />
			</layout>
		</appender>

		<logger name="TactTime" additivity="false">
			<level value="ALL" />
			<appender-ref ref="TactTimeAppender" />
		</logger>
		<appender name="TactTimeAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_TactTime.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%date{yyyy/MM/dd HH:mm:ss fff}] [TID:%thread]: %message%newline" />
			</layout>
		</appender>

		<logger name="Sequence" additivity="false">
			<level value="ALL" />
			<appender-ref ref="SequenceAppender" />
		</logger>
		<appender name="SequenceAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_Sequence.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%date{yyyy/MM/dd HH:mm:ss fff}] [TID:%thread]: %message%newline" />
			</layout>
		</appender>

		<logger name="DataBase" additivity="false">
			<level value="ALL" />
			<appender-ref ref="DataBaseAppender" />
		</logger>
		<appender name="DataBaseAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datepattern value="yyyy\\\\MM\\\\dd\\\\yyyy_MM_dd'_DataBase.log'"/>
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%date{yyyy/MM/dd HH:mm:ss fff}] [TID:%thread]: %message%newline" />
			</layout>
		</appender>

	</log4net>
</configuration>